<?xml version="1.0"?>
<package>
  <name>brisk_new</name>
  <version>0.0.0</version>
  <description>The brisk package</description>

  <!-- One maintainer tag required, multiple allowed, one person per tag --> 
  <maintainer email="slynen@ethz.ch">slynen</maintainer>
  <license>New BSD</license>

  <!-- Author tags are optional, mutiple are allowed, one per tag -->
  <!-- Authors do not have to be maintianers, but could be -->
  <maintainer email="sleutenegger@ethz.ch">sleutenegger</maintainer>
  <maintainer email="slynen@ethz.ch">slynen</maintainer>

  <!-- Add your dependencies using the build_depend tag -->
  <build_depend>agast_new</build_depend>
  <build_depend>features_2d_helpers</build_depend>
  <build_depend>glog_catkin</build_depend>
  
  <run_depend>agast_new</run_depend>
  <run_depend>features_2d_helpers</run_depend>
  <run_depend>glog_catkin</run_depend>

  <!-- Use buildtool_depend for build tool packages: -->
  <buildtool_depend>catkin</buildtool_depend>
  <buildtool_depend>redwood_catkin</buildtool_depend>
</package>
